function [ nbBarError, nbSaumonError] = evaluateClassifier(TestBar,TestSaumon,muBar,sigmaBar,muSaumon,sigmaSaumon)
%UNTITLED2 Summary of this function goes here
%   Detailed explanation goes here
ResBar = classifieurMV(TestBar,muBar,sigmaBar,muSaumon,sigmaSaumon);
ResSaumon = classifieurMV(TestSaumon,muBar,sigmaBar,muSaumon,sigmaSaumon);

%nbBarError = 2*size(ResBar,1)-sum(ResBar);
%nbSaumonError = sum(ResSaumon)-size(ResSaumon,1);
nbBarError = sum(ResBar)/size(TestBar,1);
nbSaumonError = sum(ResSaumon)/size(TestSaumonsaumon,1);
end